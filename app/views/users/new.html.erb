<%= render 'shared/form_error', obj: @user %>

<%= form_for @user do |f| %>
	<%= render 'shared/form_error', obj: @user %>
	<div>
		<%= f.label :username %>
		<%= f.text_field :username %>
	</div>
	<div>
		<%= f.label :password %>
		<%= f.password_field :password %>
	</div>
	<%= f.submit(@user.new_record? ? "Register" : "Update Profile", class: "btn btn-primary")%>
<% end %>
